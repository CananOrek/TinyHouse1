<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.demo.EvSahibiReservationController" prefWidth="700" prefHeight="400">
    <children>
        <TableView fx:id="reservationTable" layoutX="25" layoutY="25" prefWidth="650" prefHeight="300">
            <columns>
                <TableColumn fx:id="colHouse" text="Ev"/>
                <TableColumn fx:id="colTenant" text="Kiracı"/>
                <TableColumn fx:id="colCheckIn" text="Giriş"/>
                <TableColumn fx:id="colCheckOut" text="Çıkış"/>
                <TableColumn fx:id="colStatus" text="Durum"/>
            </columns>
        </TableView>
        <Button layoutX="180" layoutY="340" text="Kabul Et" onAction="#handleApprove"/>
        <Button layoutX="380" layoutY="340" text="Reddet" onAction="#handleReject"/>
    </children>
</AnchorPane>
